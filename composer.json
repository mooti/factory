{
    "name":"mooti/testable",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": "5.1.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Mooti\\Testable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mooti\\Test\\PHPUnit\\Testable\\": "tests/phpunit/"
        }
    },
    "config": {
        "bin-dir": "bin/vendor/"
    }
}