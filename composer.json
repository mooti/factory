{
    "name":"mooti/factory",
    "description": "Small library for helping create testable code",
    "keywords": ["testing", "factory", "testable", "mock"],
    "type": "library",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": "5.1.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Mooti\\Factory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mooti\\Test\\PHPUnit\\Factory\\": "tests/phpunit/"
        }
    },
    "config": {
        "bin-dir": "bin/vendor/"
    }
}